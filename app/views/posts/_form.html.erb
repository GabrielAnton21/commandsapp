<br><br><br><br>
<%= form_with(model: post) do |form| %>
  <% if post.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(post.errors.count, "error") %> prohibited this post from being saved:</h2>

      <ul>
        <% post.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :heading %>
    <%= form.text_field :heading %>
  </div>

  <div class="field">
    <%= form.label :subheading %>
    <%= form.text_field :subheading %>
  </div>

  <div class="field">
    <%= form.label :text %>
    <%= form.text_field :text %>
  </div>

  <div class="field">
    <%= form.label :link %>
    <%= form.text_field :link %>
  </div>

  <div class="field">
    <%= form.label :buttontext %>
    <%= form.text_field :buttontext %>
  </div>

  <div class="field">
    <%= form.label :imagelink %>
    <%= form.text_field :imagelink %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

